{
    "name": "genealabs/laravel-nova-morph-many-to-one",
    "description": "Drop-in package that adds a many-to-one (inverse of one-to-many) polymorphic relationship to Eloquent and Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "illuminate/database": "^7.0",
        "illuminate/support": "^7.0",
        "laravel/nova": "^3.0",
        "symfony/thanks": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpmd/phpmd": "^2.8",
        "phpstan/phpstan": "^0.12.33",
        "phpunit/phpunit": "^8.5",
        "sebastian/phpcpd": "^5.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelNovaMorphManyToOne\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelNovaMorphManyToOne\\Providers\\Service"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
